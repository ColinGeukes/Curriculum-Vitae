doctype html
html
    head
        title #{appName}
        link(href="https://fonts.googleapis.com/css?family=Maven+Pro&display=swap" rel="stylesheet")
        script(type="text/javascript", src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js")
        script(type="text/javascript", src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.12.1/jquery-ui.min.js")
        script(src="https://kit.fontawesome.com/62bb281db1.js")
        link(rel="stylesheet", type="text/css", href="/css/index.css")
        link(rel="stylesheet", type="text/css", href="/css/pageThanks.css")
    body
        include includes/navigation
        div(class="content-container")
            section(id="project" class="central-section")
                div(class="section-header clip-always-visible has-shadow")
                    div(class="content")
                        h1 Thank you!
                    div(class="shadow")
                div(class="section-body straight")
                    div(class="content")
                        h2 Thank you #{params.name},
                        p for getting in contact with me. <br> I will mostly reply within a week. <br> Future information is also sent by email.
                        p Your sincerely, <br> Colin Geukes.
                    div(class="shadow")
        include includes/footer